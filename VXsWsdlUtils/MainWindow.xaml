        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VXsWsdlUtils"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tb_wsdlPath" HorizontalAlignment="Left" Margin="10,10,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="359"/>
        <Button x:Name="bt_LoadWsdl" Content="Button" HorizontalAlignment="Left" Margin="374,9,0,0" VerticalAlignment="Top" Click="bt_LoadWsdl_Click"/>

    </Grid>
</Window>
